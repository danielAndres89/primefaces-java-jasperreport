<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>menuPrincipal</managed-bean-name>
		<managed-bean-class>com.cid.beans.MenuPrincipal</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>usuarioDataManager</managed-bean-name>
		<managed-bean-class>com.cid.beans.UsuarioDataManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>controladorLogin</managed-bean-name>
		<managed-bean-class>com.cid.beans.ControladorLogin</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>plantillaController</managed-bean-name>
		<managed-bean-class>com.cid.beans.PlantillaController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>reporteBeanOC</managed-bean-name>
		<managed-bean-class>com.cid.beans.ReporteBeanOC</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>reporteBeanSP</managed-bean-name>
		<managed-bean-class>com.cid.beans.ReporteBeanSP</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

</faces-config>
